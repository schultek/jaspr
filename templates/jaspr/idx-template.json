{
  "name": "Jaspr",
  "description": "A web project template for Jaspr",
  "icon": "https://raw.githubusercontent.com/schultek/jaspr/refs/heads/main/assets/logo_emoji.png",
  "categories": [
    "Jaspr",
    "Web app"
  ],
  "publisher": "Kilian Schulte",
  "params": [
    {
      "id": "mode",
      "name": "Rendering Mode",
      "type": "enum",
      "default": "static:auto",
      "options": {
        "static:auto": "Static Site (Recommended)",
        "server:auto": "Server Rendered Site",
        "client": "Single Page Application"
      },
      "required": true
    },
    {
      "id": "routing",
      "name": "Routing Strategy",
      "type": "enum",
      "default": "multi-page",
      "options": {
        "multi-page": "Multi-page Routing",
        "single-page": "Single-page Routing",
        "none": "No Routing"
      },
      "required": false
    },
    {
      "id": "flutter",
      "name": "Setup Flutter Embedding",
      "type": "boolean",
      "default": "false",
      "required": false
    },
    {
      "id": "plugins",
      "name": "Enable support for Flutter web plugins",
      "type": "boolean",
      "default": "false",
      "required": false
    }
  ]
}